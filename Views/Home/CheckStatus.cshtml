@{
    ViewData["Title"] = "CheckStatus";
    var test = "";
    if (ViewBag.Msg != null)
    {
        test = ViewBag.Msg.ToString();
    }
}

<h1>CheckStatus</h1>

<h4>CheckStatus</h4>
<hr />
<div class="row">
    @Html.AntiForgeryToken();
    <div class="col-md-4">
        <form asp-action="CheckStatus">
            <div class="form-group">
                <label class="control-label">Tracking Id</label>
                <input name="TrackingId" class="form-control" />
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script>
        $(document).ready(function () {
            var a = "@test.ToString()";
            if (a !== "") {
                alert(a);
            }
        })
    </script>

}
